    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.21.1",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.16",
    "path": "^0.12.7",
    "php-server": "^2.0.0",
    "process": "^0.11.10",
    "serve": "^14.2.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.0",
