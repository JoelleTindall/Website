{
  "name": "joellesite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "prisma": {
    "schema": "./frontend/src/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "concurrently \"dotenv -e .env.development -- vite\" \"dotenv -e .env.development -- node backend/server.js\"",
